{

  "$id": "https://example.com/address.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "login",
  "description": "login object for POST response",
  "type": "object",
  "properties": {
    "requestType": {
      "description": "the object type should be login",
      "type": "string",
      "pattern": "^login$"
    },
    "username": {
      "description": "the user's username",
      "type": "string",
      "pattern": "^[a-zA-Z0-9_ ]+$"
    },
    "password": {
      "description": "the user's password",
      "type": "string",
      "pattern": "^[a-zA-Z0-9_ ]+$"
    },
    "userID": {
      "description": "the matching userID",
      "type": "integer"
    }
  },
  "required": ["requestType", "username", "password", "userID"],
  "additionalProperties": false
}
