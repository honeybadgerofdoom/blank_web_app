{
  "$id": "https://example.com/address.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "game",
  "description": "login object for POST request",
  "type": "object",
  "properties": {
    "requestType": {
      "description": "the object type should be login",
      "type": "string",
      "pattern": "^game$"
    },
    "userID": {
      "description":"a username.",
      "type":"number"
    },
    "gameID": {
      "description":"a username.",
      "type":"number"
    }
  },
  "required":["requestType", "userID", "gameID"],
  "additionalProperties": true
}
